      "blur_params": [{"sigma": [0, 1, 1]}],
      "simple_params": [{
                "mirror_only": [1, 2],
                "transpose_only":[1, 2]
      }],
      "noise_params": [{
	      "var": 0.00001
      }],
      "blur": [true],
      "simple": [true],
      "noise": [true],
      "norm_factor": [0.001],
      "save_every": [1],

      "point_density": [0.00000001, 0.1]
  },
  "seg": {
      "data_file": ["data/ctc/17_1A.zarr"],
      "dataset": [{
	      "train": {"raw": "train/raw", "gt": "train/gt"},
	      "validate": {"raw": "validate/raw", "gt": "validate/gt"}
      	}
      ],
      "batch_size": [4],
      "optimizer": ["adam"],
      "lr": [1e-5],
      "epochs": [3],
