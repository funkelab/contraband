{
  "contrastive": {
      "data_file": ["data/ctc/Fluo-N2DH-SIM+.zarr"],
      "dataset": ["train/raw"],
      "optimizer": ["adam"],
      "lr": [1e-5],
      "epochs": [3],
      "num_iterations": [2],
      "elastic": [true],
      "elastic_params": [{
	"control_point_spacing": [1, 10, 10],
	"jitter_sigma": [0.0, 0.1, 0.1],
	"rotation_interval": [0, "pi / 2"]
      }],
      "blur_params": [{"sigma": [0, 1, 1]}],
      "simple_params": [{
