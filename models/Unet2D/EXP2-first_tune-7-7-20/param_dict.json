                "transpose_only":[1, 2]
      }],
      "noise_params": [{
	      "var": 0.000001
      }],
      "blur": [true],
      "simple": [true],
      "noise": [true],
      "norm_factor": [0.25],
      "save_every": [21],

      "point_density": [0.01, 0.1],

      "h_channels": [20],
      "out_channels": [3, 6, 9]
  },
  "seg": {
      "data_file": ["data/ctc/Fluo-N2DH-SIM+.zarr"],
      "dataset": [{
	      "train": {"raw": "train/raw", "gt": "train/gt"},
	      "validate": {"raw": "validate/raw", "gt": "validate/gt"}
      	}
      ],
      "batch_size": [4],
      "optimizer": ["adam"],
      "lr": [1e-5],
      "epochs": [3],
      "num_iterations": [4],
      "elastic": [true],
      "blur": [true],
      "simple": [false],
      "noise": [false],
      "seg_head": ["SimpleSegHead"],
      "thresholds": [[0.5, 0.75]],
      "norm_factor": [0.25],
      "save_every": [3],

      "h_channels": [20],
      "out_channels": [2]
  }
